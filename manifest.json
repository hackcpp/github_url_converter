{
  "manifest_version": 3,
  "name": "GitHub URL转换器",
  "version": "1.0",
  "description": "一键将GitHub页面切换到DeepWiki或GitHub1s",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_popup": "src/popup/popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png"
    }
  },
  "permissions": ["tabs"],
  "host_permissions": [
    "*://github.com/*",
    "*://github1s.com/*",
    "*://deepwiki.com/*"
  ]
}